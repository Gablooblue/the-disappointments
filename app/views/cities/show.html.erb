<div class = "container" id = "padding-default-top">
    <div class = "col-md-12">
	<div class = "card">
	    <div class = "card-body">
		<h3 class = "card-title">
		    <%=  @city.name  %>
		</h3>
	    </div>
	</div>
    <h4 class = "text-center py-4">Delicacies</h4>
    <div class = "row">
	<% @city.delicacies.each do |delicacy| %>
		<%= render 'delicacies/delicacy', delicacy: delicacy%>
	<% end %>
    </div>

</div>
