<div class = "container" id = "padding-default-top">
    <div class = "col-md-12">
	<div class = "card">
	    <div class = "card-body">
		<h3 class = "card-title">
		    <%= @city.name %>
		</h3>
	    </div>
	</div>
    </div>
    <h4 class = "text-center">Delicacies</h4>
    <% @city.delicacies.each do |delicacy| %>
	<%= render 'delicacies/delicacy', delicacy: delicacy%>
    <% end %>
</div>
